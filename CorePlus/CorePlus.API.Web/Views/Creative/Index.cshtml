@{
    ViewBag.Title = "Index";
}
<script src="@Url.Content("~/Js/Baidu/Creative/index.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Js/Baidu/changetab.js")" type="text/javascript"></script>
<input id="SelectedId" name="SelectedId" type="hidden" value="@ViewBag.HighId" />
<div class="manage_bulletin scroll_y_top_fixed" style="z-index: 0; top: 0px; position: relative;">
    <div class="ui_mcal">
        <select id="ddlDatePick" class="easyui-combobox" data-options="editable:false, panelHeight: 'auto',value:'1'">
            <option value="1">昨天</option>
            <option value="2">本周</option>
            <option value="3">本月</option>
        </select>
        &nbsp;&nbsp;<a id="keywordA" href="#">关键词</a>&nbsp;&nbsp;<a id="sublinkA" href="#">附加创意</a>
    </div>
    <span class="ui_bubble">&nbsp;<span class="ui_bubble_icon_none">&nbsp;</span></span>
    <ul class="bulletin">
        <li>
            <h3 id="sRecordCount" class="ui_label">
                0</h3>
            <span class="bulletin-char">创意总数</span> </li>
        <li>
            <h3 id="sShowCnt" class="ui_label">
                0</h3>
            <span class="bulletin-char">展现</span> </li>
        <li>
            <h3 id="sClicked" class="ui_label">
                0</h3>
            <span class="bulletin-char">点击</span> </li>
        <li>
            <h3 id="sTotalCost" class="ui_label" title="¥0.00">
                ¥0.00</h3>
            <span class="bulletin-char">消费</span> </li>
        <li>
            <h3 id="sTransformCnt" class="ui_label">
                0</h3>
            <span class="bulletin-char">转化</span> </li>
        <li>
            <h3 id="sClickedRate" class="ui_label">
                0.00%</h3>
            <span class="bulletin-char">点击率</span> </li>
        <li>
            <h3 id="sAvgClickedPrice" class="ui_label">
                ¥0.00</h3>
            <span class="bulletin-char">平均点击</span> </li>
    </ul>
</div>
<div class="manage_table">
    <table id="dg" border="0">
    </table>
</div>
<div id="addDiv" style="display: none">
    <iframe id="frameAdd" src="javascript:void(0)" frameborder="0" width="100%" height="100%">
    </iframe>
</div>