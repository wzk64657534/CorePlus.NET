@{
    ViewBag.Title = "Online";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">
    $(function () {
        $('#tt').attr("width", document.body.clientWidth * 0.9);
    });
    function addTab(title, url) {
        var content = '<iframe scrolling="auto" frameborder="0"  src="' + url + '" style="width:100%;height:100%;"></iframe>';
        var i = Number($('#tt li').length) + 1;

        $('#tt').tabs('add', {
            title: "客服窗口" + i + "号",
            content: content,
            closable: true,
            width: function () { return document.body.clientWidth * 0.9 }
        });
    }
</script>
<div style="margin: 10px;">
    <input type="button" name="name" value="添加窗口" onclick="addTab('google','@ViewBag.ServantUrl')" style="width: 60px;height: 30px" />
</div>
<div id="tt" class="easyui-tabs" style="height: 650px;">
</div>
