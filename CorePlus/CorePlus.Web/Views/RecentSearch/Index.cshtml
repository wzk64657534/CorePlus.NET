                    <li class="item" tag="5" info="-4">&nbsp;<a href="javascript:void(0);"></a></li>
                    <li class="item" tag="6" info="-5">&nbsp;<a href="javascript:void(0);"></a></li>
                    <li class="item" tag="7" info="-6">&nbsp;<a href="javascript:void(0);"></a></li>
                </ul>
            </div>
            <div class="choice">
                <div class="choice-right">
                    <div class="fleft">
                        <span>输入完整IP：</span><input id="txbSearch" type="text" style="color: #ccc; width: 100px;"
                            class="util_search_input1 lightGray" /></div>
                    <div class="button fleft">
                        <a class="filter_button1" id="btnSearch">搜索</a></div>
                </div>
            </div>
            <!-- 受访页面内容 -->
            <div class="tab-content" id="zhannei_shoufang_content" style="display: block;">
                <table id="dg" border="0">
